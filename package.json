{
  "name": "@dankreiger/groop",
  "version": "0.1.22",
  "description": "Normalize an array of objects by a property.",
  "homepage": "https://github.com/dankreiger/groop#readme",
  "bugs": {
    "url": "https://github.com/dankreiger/groop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankreiger/groop.git"
  },
  "license": "MIT",
  "author": "Dan Kreiger",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "scripty",
    "clean": "scripty",
    "lint": "scripty",
    "prepare": "scripty",
    "semantic-release": "semantic-release",
    "size": "scripty",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/src/*.{js,ts,tsx,jsx,json,css}": [
      "yarn sort-package-json && prettier --single-quote --write"
    ],
    "*.{md,mdx,yml}": [
      "yarn sort && prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime": "7.18.6",
    "@commitlint/config-conventional": "17.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@size-limit/preset-small-lib": "7.0.8",
    "@size-limit/webpack-why": "7.0.8",
    "@types/jest": "28.1.8",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "0.32.1",
    "scripty": "^2.1.0",
    "semantic-release": "19.0.5",
    "size-limit": "7.0.8",
    "sort-package-json": "^1.57.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "size-limit": [
    {
      "path": "dist/es/index.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/cjs/index.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "4 KB"
    }
  ]
}
