{
  "name": "@dankreiger/groop",
  "version": "0.1.19",
  "description": "Normalize an array of objects by a property.",
  "homepage": "https://github.com/dankreiger/groop#readme",
  "bugs": {
    "url": "https://github.com/dankreiger/groop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankreiger/groop.git"
  },
  "license": "MIT",
  "author": "Dan Kreiger",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "scripty",
    "clean": "scripty",
    "lint": "scripty",
    "prepare": "scripty",
    "semantic-release": "semantic-release",
    "size": "scripty",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/src/*.{js,ts,tsx,jsx,json,css}": [
      "yarn sort-package-json && prettier --single-quote --write"
    ],
    "*.{md,mdx,yml}": [
      "yarn sort && prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@size-limit/preset-small-lib": "7.0.8",
    "@size-limit/webpack-why": "7.0.8",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.0",
    "scripty": "^2.1.0",
    "semantic-release": "19.0.2",
    "size-limit": "7.0.8",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "tslib": "2.4.0",
    "typescript": "4.7.2"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "size-limit": [
    {
      "path": "dist/es/index.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/cjs/index.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "4 KB"
    }
  ]
}
